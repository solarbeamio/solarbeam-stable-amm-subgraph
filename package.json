{
  "name": "solarbeam-stable-subgraph",
  "license": "GPLv3",
  "scripts": {
    "lint": "eslint --max-warnings=0 src/ && prettier --list-different src/**/*.ts",
    "lint:fix": "eslint src/ --fix && prettier --write src/**/*.ts",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ solarbeam/stable",
    "create-local": "graph create --node http://localhost:8020/ solarbeam/stable",
    "remove-local": "graph remove --node http://localhost:8020/ solarbeam/stable",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 solarbeam/stable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saddle-finance/saddle-subgraph.git"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.20.1",
    "@graphprotocol/graph-ts": "0.20.1",
    "@protofire/subgraph-toolkit": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
